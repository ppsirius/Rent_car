<%= simple_form_for(@car) do |f| %>
	<%= f.association :type, placeholder: "Type" %>
	<%= f.input :plate, placeholder: "Plate" %>
	<%= f.input :production_year, placeholder: "Production year" %>
	<%= f.input :car_paint, placeholder: "Color"%>
	<%= f.input :state, collection: ["available","rented", "clean", "serviced"], default:"available"%>
	<%= f.button(:submit) %>
<% end %>
